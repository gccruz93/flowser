{
  "css.lint.unknownAtRules": "ignore",
  "editor.linkedEditing": true,
  "editor.tabSize": 2,
  "eslint.validate": [
    "javascript"
  ],
  "files.eol": "\n",
  "[html][css][javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true,
    "editor.tabSize": 2,
    "editor.codeActionsOnSave": {
      "source.fixAll.eslint": "explicit"
    }
  }
}